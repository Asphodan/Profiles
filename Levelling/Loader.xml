<HBProfile>
<Name>Bot Loader</Name>
<QuestOrder>
<If Condition="Me.IsAlliance" >
	<If Condition="Me.Race == WoWRace.Human">
		<If Condition="(Me.Level &gt;= 15)" >
			<CustomBehavior File="LoadProfile" ProfileName="..\Levelling\1-60\Alliance\Eastern Kingdoms\Redridge" />
		</If>
		<If Condition="(Me.Level &gt;= 10)" >
			<CustomBehavior File="LoadProfile" ProfileName="..\Levelling\1-60\Alliance\Eastern Kingdoms\Westfall" />
		</If>
		<If Condition="(Me.Level &gt;= 5)" >
			<CustomBehavior File="LoadProfile" ProfileName="..\Levelling\1-60\Alliance\Eastern Kingdoms\Elwynn" />
		</If>
		<If Condition="(Me.LevelFraction &gt;= 1)" >
			<CustomBehavior File="LoadProfile" ProfileName="..\Levelling\1-60\Alliance\Eastern Kingdoms\Northshire" />
		</If>
	</If>
</If>
</QuestOrder>
</HBProfile>